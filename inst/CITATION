year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{eplusr}: A Toolkit for Using EnergyPlus in R",
         author = c(person("Hongyuan", "Jia")),
         year = year,
         note = note,
         url = "https://CRAN.R-project.org/package=eplusr")
